{
    "functions": {
        "index.php": {
            "runtime": "vercel-php@0.7.3"
        },
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/$1"
        }
    ]
}